<animated-vector
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt">
    <aapt:attr name="android:drawable">
        <vector
            android:name="vector"
            android:width="24dp"
            android:height="24dp"
            android:viewportWidth="24"
            android:viewportHeight="24">
            <path
                android:name="bg"
                android:pathData="M 7 1 C 6.47 1 5.961 1.211 5.586 1.586 C 5.211 1.961 5 2.47 5 3 L 5 21 C 5 21.53 5.211 22.039 5.586 22.414 C 5.961 22.789 6.47 23 7 23 L 17 23 C 17.53 23 18.039 22.789 18.414 22.414 C 18.789 22.039 19 21.53 19 21 L 19 3 C 19 2.47 18.789 1.961 18.414 1.586 C 18.039 1.211 17.53 1 17 1 L 7 1 M 7 4 L 17 4 L 17 20 L 7 20 L 7 4 Z"
                android:fillColor="#000"/>
            <group android:name="top_group">
                <path
                    android:name="top"
                    android:pathData="M 9 6 L 9 10 L 10.5 10 L 10.5 7.5 L 13 7.5 L 13 6 L 9 6 Z"
                    android:fillColor="#000000"/>
            </group>
            <group android:name="bottom_group">
                <path
                    android:name="bottom"
                    android:pathData="M 13.5 14 L 13.5 16.5 L 11 16.5 L 11 18 L 15 18 L 15 14 L 13.5 14 Z"
                    android:fillColor="#000000"/>
            </group>
        </vector>
    </aapt:attr>
    <target android:name="top_group">
        <aapt:attr name="android:animation">
            <set>
                <objectAnimator
                    android:propertyName="translateX"
                    android:startOffset="250"
                    android:duration="250"
                    android:valueFrom="-2"
                    android:valueTo="0"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/accelerate_decelerate_interpolator"/>
                <objectAnimator
                    android:propertyName="translateY"
                    android:startOffset="250"
                    android:duration="250"
                    android:valueFrom="-2"
                    android:valueTo="4"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/accelerate_decelerate_interpolator"/>
                <objectAnimator
                    android:propertyName="translateX"
                    android:startOffset="500"
                    android:duration="500"
                    android:valueFrom="0"
                    android:valueTo="0"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/accelerate_decelerate_interpolator"/>
                <objectAnimator
                    android:propertyName="translateY"
                    android:startOffset="500"
                    android:duration="500"
                    android:valueFrom="4"
                    android:valueTo="0"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/accelerate_decelerate_interpolator"/>
                <objectAnimator
                    android:propertyName="translateX"
                    android:duration="250"
                    android:valueFrom="0"
                    android:valueTo="-2"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/accelerate_decelerate_interpolator"/>
                <objectAnimator
                    android:propertyName="translateY"
                    android:duration="250"
                    android:valueFrom="0"
                    android:valueTo="-2"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/accelerate_decelerate_interpolator"/>
            </set>
        </aapt:attr>
    </target>
    <target android:name="bottom_group">
        <aapt:attr name="android:animation">
            <set>
                <objectAnimator
                    android:propertyName="translateX"
                    android:startOffset="250"
                    android:duration="250"
                    android:valueFrom="2"
                    android:valueTo="0"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/accelerate_decelerate_interpolator"/>
                <objectAnimator
                    android:propertyName="translateY"
                    android:startOffset="250"
                    android:duration="250"
                    android:valueFrom="2"
                    android:valueTo="-4"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/accelerate_decelerate_interpolator"/>
                <objectAnimator
                    android:propertyName="translateX"
                    android:startOffset="500"
                    android:duration="500"
                    android:valueFrom="0"
                    android:valueTo="0"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/accelerate_decelerate_interpolator"/>
                <objectAnimator
                    android:propertyName="translateY"
                    android:startOffset="500"
                    android:duration="500"
                    android:valueFrom="-4"
                    android:valueTo="0"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/accelerate_decelerate_interpolator"/>
                <objectAnimator
                    android:propertyName="translateX"
                    android:duration="250"
                    android:valueFrom="0"
                    android:valueTo="2"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/accelerate_decelerate_interpolator"/>
                <objectAnimator
                    android:propertyName="translateY"
                    android:duration="250"
                    android:valueFrom="0"
                    android:valueTo="2"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/accelerate_decelerate_interpolator"/>
            </set>
        </aapt:attr>
    </target>
</animated-vector>
